{
  "$id": "tunable_types",
  "title": "Tunable Types",
  "type": "array",
  "default": [],
  "items": {
    "title": "Tunable Definition",
    "type": "object",
    "required": [
      "key",
      "type",
      "display"
    ],
    "properties": {
      "key": {
        "title": "Tunable Hash",
        "type": "string",
        "oneOf": [
          {
            "pattern": "^0x[A-F0-9]{8}$"
          },
          {
            "pattern": "^[A-Z0-9_]+$"
          }
        ],
        "examples": [
          "0xDD8B0260",
          "XM22_BIN_PRICE_BRICKADE2"
        ]
      },
      "type": {
        "title": "Tunable Type",
        "type": "string",
        "enum": [
          "avenger_upgrade_sales",
          "biker_business_sales",
          "biker_clubhouse_sales",
          "bunker_sales",
          "nightclub_property_sales",
          "nightclub_upgrade_sales",
          "office_sales",
          "property_sales",
          "vehicle_sales",
          "vehicle_upgrade_sales",
          "weapon_sales"
        ]
      },
      "display": {
        "title": "Display Hash",
        "type": "string",
        "oneOf": [
          {
            "pattern": "^0x[A-F0-9]{8}$"
          },
          {
            "pattern": "^[A-Z0-9_]+$"
          }
        ],
        "examples": [
          "0xAFB83BD5",
          "BRICKADE2"
        ]
      }
    }
  }
}